<Window x:Class="HCI_Project1_FootballLeague.ChooseSeasonWindow.SeasonChooserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_Project1_FootballLeague.ChooseSeasonWindow"
        mc:Ignorable="d"
        Title="Season Chooser Window" Height="229" Width="385" MinHeight="215" MinWidth="384">
    <Grid Margin="0,0,10,-6">
        <ComboBox x:Name="ChooseSeasonBox" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,57,0,0" Width="169"/>
        <Button x:Name="NextButton" Click="NextButton_Click" Content="Next" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,148,0,0" Height="36" Width="172"/>

    </Grid>
</Window>
