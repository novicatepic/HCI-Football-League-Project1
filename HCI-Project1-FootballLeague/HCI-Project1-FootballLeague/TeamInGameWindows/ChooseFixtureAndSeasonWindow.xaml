<Window x:Class="HCI_Project1_FootballLeague.TeamInGameWindows.ChooseFixtureAndSeasonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_Project1_FootballLeague.TeamInGameWindows"
        mc:Ignorable="d"
        Title="Choose Fixture And Season Window" Height="450" Width="800">
    <Grid Margin="0,0,10,-6">
        <ComboBox SelectionChanged="ChooseSeasonBox_SelectionChanged" x:Name="ChooseSeasonBox" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,88,0,0" Width="170"/>
        <ComboBox x:Name="ChooseFixtureBox" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,167,0,0" Width="170"/>
        <Button x:Name="NextButton" Click="NextButton_Click" Content="Next" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="312,282,0,0" Height="36" Width="172"/>
        <Label Content="Choose Fixture" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="14" HorizontalAlignment="Center" Margin="0,127,0,0" VerticalAlignment="Top" Width="166" Height="35"/>
        <Label Content="Choose Season" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="14" HorizontalAlignment="Left" Margin="314,48,0,0" VerticalAlignment="Top" Width="166" Height="35"/>

    </Grid>
</Window>
