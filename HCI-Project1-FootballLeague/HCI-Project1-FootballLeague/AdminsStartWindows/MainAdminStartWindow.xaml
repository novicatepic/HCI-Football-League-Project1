<Window x:Class="HCI_Project1_FootballLeague.AdminsStartWindows.MainAdminStartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_Project1_FootballLeague.AdminsStartWindows"
        mc:Ignorable="d"
        Title="Main Admin Start Window" Height="500" Width="800" MinHeight="500" MinWidth="800">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Margin="10 10 10 10" x:Name="ShowLeagueAdminsButton" Content="Show League Administrators" Height="60" Width="350" Click="Button_Click"/>
        <Separator></Separator>
        <Button Margin="10 10 10 10" x:Name="ShowOptionsButton" Content="Show Options" Height="60" Width="350" Click="ShowOptionsButton_Click"/>

    </StackPanel>
</Window>
